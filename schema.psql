--
-- Name: GREENHOUSE_GASES_VIEW; Type: VIEW; Schema: public; Owner: echoepa
--

CREATE VIEW public."GREENHOUSE_GASES_VIEW" AS
 SELECT re."REPORTING_YEAR",
    re."REGISTRY_ID",
    re."PGM_SYS_ACRNM",
    re."PGM_SYS_ID",
    re."POLLUTANT_NAME",
    re."ANNUAL_EMISSION",
    re."UNIT_OF_MEASURE",
    re."NEI_TYPE",
    re."NEI_HAP_VOC_FLAG",
    ee."FAC_NAME",
    ee."FAC_STREET",
    ee."FAC_CITY",
    ee."FAC_STATE",
    ee."FAC_ZIP",
    ee."FAC_COUNTY",
    ee."FAC_EPA_REGION",
    ee."FAC_LAT",
    ee."FAC_LONG",
    ee."FAC_DERIVED_WBD",
    ee."FAC_DERIVED_CD113",
    ee."FAC_PERCENT_MINORITY",
    ee."FAC_POP_DEN",
    ee."FAC_DERIVED_HUC",
    ee."FAC_SIC_CODES",
    ee."FAC_NAICS_CODES",
    ee."DFR_URL"
   FROM public."POLL_RPT_COMBINED_EMISSIONS" re,
    public."ECHO_EXPORTER" ee
  WHERE (((re."REGISTRY_ID")::text = (ee."REGISTRY_ID")::text) AND (re."PGM_SYS_ACRNM" = 'E-GGRT'::text));


